<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="domain">
	<class name="Pyrz" table="tb_pyrz">
		<id name="id" column="id" type="integer">
			<generator class="increment" />
		</id>
		<property name="lw" column="va_lw" type="integer" />
		<property name="gx" column="va_gx" type="integer" />
		<property name="xuehao" column="va_xuehao" length="20" type="string" />
		<property name="zuhao" column="va_zuhao" type="integer" />
		<property name="gonghao" column="va_gonghao" length="20" type="string" />
		<property name="time" column="va_time" type="timestamp" />
		<property name="text" column="va_text" type="text" />
		<property name="fs" column="va_fs" type="float" />
		<property name="year" column="va_year" type="integer" />
		<property name="xy" column="va_xy" type="integer" />
	</class>
</hibernate-mapping>