<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="domain">
	<class name="Notices" table="tb_notices">
		<id name="id" column="id" type="integer">
			<generator class="increment" />
		</id>
		<property name="noticeId" column="va_notice" type="integer" />
		<property name="fileName" column="va_filename" length="100" type="string" />
		<property name="url" column="va_url" length="30" type="string" />
		<property name="year" column="va_year" type="integer" />
		<property name="status" column="va_status" type="integer" />
	</class>
</hibernate-mapping>