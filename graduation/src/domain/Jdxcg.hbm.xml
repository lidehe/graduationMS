<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="domain">
	<class name="Jdxcg" table="tb_jdxcg">
		<id name="id" column="id" type="integer">
			<generator class="increment" />
		</id>
		<property name="xuehao" column="va_xs" length="20" type="string" />
		<property name="status" column="va_status" type="integer" />
		<property name="fileName" column="va_filename" length="100" type="string" />
	</class>
</hibernate-mapping>