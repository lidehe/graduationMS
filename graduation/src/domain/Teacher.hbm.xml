<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="domain">
	<class name="Teacher" table="tb_teacher">
		<id name="id" column="id" type="integer">
			<generator class="increment" />
		</id>
		<property name="number" column="va_number" length="20" type="string" />
		<property name="name" column="va_name" length="30" type="string" />
		<property name="sex" column="va_sex" type="integer" />
		<property name="identityN" column="va_identity" length="20" type="string" />
		<property name="yx" column="va_yxid" type="integer" />
		<property name="tel" column="va_telephone" length="20" type="string" />
		<property name="email" column="va_email" length="30" type="string" />
		<property name="year" column="va_year" type="integer" />
		<property name="fzr" column="va_fzr" type="integer" />
	</class>
</hibernate-mapping>