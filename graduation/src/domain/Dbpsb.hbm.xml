<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="domain">
	<class name="Dbpsb" table="tb_dbpsb">
		<id name="id" column="id" type="integer">
			<generator class="increment" />
		</id>
		<property name="gonghao" column="va_gonghao" length="20" type="string" />
		<property name="xuehao" column="va_xuehao" length="20" type="string" />
		<property name="dbpsbx" column="va_dbpsbx" type="integer" />
		<property name="fs" column="va_fs" type="float" />
		<property name="text" column="va_text" type="text" />
	</class>
</hibernate-mapping>