<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="domain">
	<class name="Account" table="tb_account">
		<id name="id" column="id" type="integer">
			<generator class="increment" />
		</id>
		<property name="account" column="va_account" length="20" type="string" />
		<property name="password" column="va_password" length="20" type="string" />
		<property name="type" column="va_type" type="integer" />
	</class>
</hibernate-mapping>