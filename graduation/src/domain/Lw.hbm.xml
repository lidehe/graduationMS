<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="domain">
	<class name="Lw" table="tb_lw">
		<id name="id" column="id" type="integer">
			<generator class="increment" />
		</id>
		<property name="xuehao" column="va_xsxh" length="20" type="string" />
		<property name="gonghao" column="va_zdls" length="20" type="string" />
		<property name="yx" column="va_yx" type="integer" />
		<property name="zy" column="va_zy" type="integer" />
		<property name="year" column="va_year" type="integer" />
		<property name="status" column="va_status" type="integer" />
		<property name="kt" column="va_kt" type="integer" />
		<property name="fileName" column="va_filename" length="100" type="string" />
	</class>
</hibernate-mapping>